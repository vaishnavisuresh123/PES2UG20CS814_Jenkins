pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        sh 'g++ -o PES2UG20CS814-1 newfile.cpp'
        echo "Build Successful"
       }
   }
   stage('Test') {
    steps {
      sh './PES2UG20CS814-1'
    }
  }
}
post {
  always {
    echo'Pipeline compleated'
  }
  failure {
    echo 'Pipeline failed'
  }
}
}
